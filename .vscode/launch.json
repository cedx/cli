{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Program",
			"preLaunchTask": "Build",
			"request": "launch",
			"type": "coreclr",
			"program": "${workspaceFolder}/bin/Belin.Cli.dll",
			"args": ["${input:command}", "${input:subcommand}"]
		}
	],
	"inputs": [
    {
			"description": "The command to invoke.",
      "id": "command",
      "type": "promptString"
    },
		{
			"description": "The subcommand to invoke.",
      "id": "subcommand",
      "type": "promptString"
    }
  ]
}
